	font-weight: 500;
	color: white;
	margin: 120rpx auto 0;
}
.box {
	padding: 15rpx;
}
.tips {
	width: 100%;
	box-sizing: border-box;
	padding: 120rpx 15rpx 0;
	text-align: center;
	color: #888;
	line-height: 200%;
}
.tips .action {
	margin-top: 60rpx;
}
.book {
	width: 206rpx;

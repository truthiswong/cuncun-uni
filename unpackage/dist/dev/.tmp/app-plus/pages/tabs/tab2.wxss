	height: 271.6rpx;
	margin-bottom: 15rpx;
}
.longpress-tips {
	border: 1rpx dashed #FF6600;
	border-radius: 3rpx;
	box-sizing: border-box;
	padding: 20rpx 20rpx 20rpx 15rpx;
	border-radius: 5rpx;
}
.longpress-tips .close-longpress-tips {
	float: right;
}
@media (min-width: 768px) {
.ellipsis-2row {
		line-height: 1.8;
}
